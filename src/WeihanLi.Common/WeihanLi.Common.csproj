<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <TargetFrameworks>netstandard2.0;net45</TargetFrameworks>
    <Product>WeihanLi.Common</Product>
    <PackageIconUrl>https://avatars3.githubusercontent.com/u/7604648</PackageIconUrl>
    <PackageId>WeihanLi.Common</PackageId>
    <Authors>WeihanLi</Authors>
    <Title>WeihanLi.Common</Title>
    <Description>common .netstandard libarary and extensions</Description>
    <Summary>common .netstandard libarary and extensions</Summary>
    <PackageRequireLicenseAcceptance>false</PackageRequireLicenseAcceptance>
    <PackageReleaseNotes>
      remove reference for AutoFac
      add reference for AspectCore
      update Configuration.ConnectionString support for .net core
      add TryInvokeInterceptor
      update AsyncLock
    </PackageReleaseNotes>
    <Copyright>Copyright 2017 (c) WeihanLi</Copyright>
    <PackageTags>common utility lib libarary extensions</PackageTags>
    <Version>1.0.9</Version>
    <PackageLicenseUrl>https://github.com/WeihanLi/WeihanLi.Common/blob/master/LICENSE</PackageLicenseUrl>
    <PackageProjectUrl>https://github.com/WeihanLi/WeihanLi.Common</PackageProjectUrl>
    <RepositoryType>git</RepositoryType>
    <RepositoryUrl>https://github.com/WeihanLi/WeihanLi.Common</RepositoryUrl>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <Optimize>true</Optimize>
  </PropertyGroup>
  <ItemGroup>
    <PackageReference Include="AspectCore.Core" Version="0.3.1" />
    <PackageReference Include="Microsoft.DotNet.Analyzers.Compatibility" Version="0.1.2-alpha" PrivateAssets="All" />
    <PackageReference Include="JetBrains.Annotations" Version="11.1.0" PrivateAssets="All" />
  </ItemGroup>
  <ItemGroup Condition="'$(TargetFramework)' == 'net45'">
    <Reference Include="Microsoft.CSharp" />
    <Reference Include="System.Configuration" />
    <Reference Include="System.Core" />
    <Reference Include="System.Data" />
    <Reference Include="System.Drawing" />
    <Reference Include="System.IO.Compression" />
    <Reference Include="System.Web" />
    <Reference Include="System.Net.Http" />
    <Compile Remove="Extensions\ConfigurationExtension.cs" />
    <Compile Remove="Log\Log4NetLoggerProvider.cs" />
    <Compile Remove="Log\MicrosoftLoggingLogHelperProvider.cs" />
  </ItemGroup>
  <ItemGroup Condition="'$(TargetFramework)' == 'netstandard2.0'">
    <PackageReference Include="Microsoft.Extensions.DependencyInjection" Version="2.0.0" />
    <PackageReference Include="Microsoft.Extensions.Logging" Version="2.0.0" />
    <PackageReference Include="Microsoft.Extensions.Configuration" Version="2.0.0" />
    <PackageReference Include="System.Configuration.ConfigurationManager" Version="4.4.0" />
  </ItemGroup>
  <ItemGroup>
    <PackageReference Include="Newtonsoft.Json" Version="10.0.3" />
    <PackageReference Include="System.ComponentModel.Annotations" Version="4.4.0" />
    <PackageReference Include="log4net" Version="2.0.8" />
  </ItemGroup>
  <ItemGroup>
    <None Include="log4net.config" Pack="true" PackagePath="content\log4net.config" />
  </ItemGroup>
  <ItemGroup>
    <Service Include="{508349b6-6b84-4df5-91f0-309beebad82d}" />
    <Compile Update="Extensions\DbCommandExtension.generated.cs">
      <DesignTime>True</DesignTime>
      <AutoGen>True</AutoGen>
      <DependentUpon>DbCommandExtension.tt</DependentUpon>
    </Compile>    
    <None Update="Extensions\DbCommandExtension.tt">
      <Generator>TextTemplatingFileGenerator</Generator>
      <LastGenOutput>DbCommandExtension.generated.cs</LastGenOutput>
    </None>
    <Compile Update="Extensions\DbConnectionExtension.generated.cs">
      <DesignTime>True</DesignTime>
      <AutoGen>True</AutoGen>
      <DependentUpon>DbConnectionExtension.tt</DependentUpon>
    </Compile>
    <None Update="Extensions\DbConnectionExtension.tt">
      <Generator>TextTemplatingFileGenerator</Generator>
      <LastGenOutput>DbConnectionExtension.generated.cs</LastGenOutput>
    </None>
    <Compile Update="Resource.Designer.cs">
      <DesignTime>True</DesignTime>
      <AutoGen>True</AutoGen>
      <DependentUpon>Resource.resx</DependentUpon>
    </Compile>
    <EmbeddedResource Update="Resource.resx">
      <Generator>ResXFileCodeGenerator</Generator>
      <LastGenOutput>Resource.Designer.cs</LastGenOutput>
    </EmbeddedResource>    
  </ItemGroup>
</Project>