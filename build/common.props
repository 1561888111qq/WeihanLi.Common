<Project>  
  <Import Project="sign.props" />
  <Import Project="version.props" />
  <Import Project="copyright.props" />
  <Import Project="repo.props" />
  <Import Project="analyzer.props" />
  <PropertyGroup>            
    <PackageId>WeihanLi.Common</PackageId>
    <Title>WeihanLi.Common</Title>
    <TargetFrameworks>netstandard2.0;net45</TargetFrameworks>    
    <Description>common netfx and netstandard libarary,extensions helpers and useful utilities</Description>
    <PackageTags>common utility lib libarary extensions helper</PackageTags>
    <PackageSummary>common netfx and netstandard libarary,extensions helpers and useful utilities</PackageSummary>
    <PackageRequireLicenseAcceptance>false</PackageRequireLicenseAcceptance>
    <PackageReleaseNotes>
      update Repository
      update AsyncLock
    </PackageReleaseNotes>
    <PackageIconUrl>https://avatars3.githubusercontent.com/u/7604648</PackageIconUrl>    
    <PackageProjectUrl>https://github.com/WeihanLi/WeihanLi.Common/tree/dev/src/WeihanLi.Common</PackageProjectUrl>
    <PackageLicenseExpression>MIT</PackageLicenseExpression>
    <Optimize Condition="'$(Configuration)'=='Release'">true</Optimize>
    <GeneratePackageOnBuild Condition="'$(Configuration)'=='Release'">True</GeneratePackageOnBuild>
    <NoWarn>$(NoWarn);1591;DE0003;</NoWarn>
  </PropertyGroup>
</Project>