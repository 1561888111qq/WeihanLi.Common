<Project>  
  <Import Project="sign.props" />
  <Import Project="version.props" />
  <Import Project="copyright.props" />
  <Import Project="repo.props" />
  <PropertyGroup>        
    <TargetFrameworks>netstandard2.0;net45</TargetFrameworks>
    <PackageIconUrl>https://avatars3.githubusercontent.com/u/7604648</PackageIconUrl>    
    <PackageProjectUrl>https://github.com/WeihanLi/WeihanLi.Common</PackageProjectUrl>
    <PackageLicenseExpression>MIT</PackageLicenseExpression>
    <PackageTags>common utility lib libarary extensions</PackageTags>    
    <Title>WeihanLi.Common</Title>
    <Description>common .netstandard libarary and extensions</Description>
    <Summary>common .netstandard libarary and extensions</Summary>
    <PackageRequireLicenseAcceptance>false</PackageRequireLicenseAcceptance>
    <PackageReleaseNotes>
      update Repository, DependencyResolver
      add GetAppSetting extension method for IConfiguration
      add GetRandomList extension method for IList
    </PackageReleaseNotes>    
    <GenerateAssemblyConfigurationAttribute>false</GenerateAssemblyConfigurationAttribute>
    <GenerateAssemblyCompanyAttribute>false</GenerateAssemblyCompanyAttribute>
    <GenerateAssemblyProductAttribute>false</GenerateAssemblyProductAttribute>
    <Optimize Condition="'$(Configuration)'=='Release'">true</Optimize>
    <GeneratePackageOnBuild Condition="'$(Configuration)'=='Release'">True</GeneratePackageOnBuild>
    <NoWarn>$(NoWarn);1591;DE0003;</NoWarn>
  </PropertyGroup>
</Project>