<Project>  
  <Import Project="sign.props" />
  <Import Project="version.props" />
  <Import Project="copyright.props" />
  <Import Project="repo.props" />
  <PropertyGroup>        
    <TargetFrameworks>netstandard2.0;net45</TargetFrameworks>
    <PackageIconUrl>https://avatars3.githubusercontent.com/u/7604648</PackageIconUrl>    
    <PackageProjectUrl>https://github.com/WeihanLi/WeihanLi.Common/tree/dev/src/WeihanLi.Common.Logging.Log4Net</PackageProjectUrl>
    <PackageLicenseExpression>MIT</PackageLicenseExpression>
    <PackageTags>logging log4net</PackageTags>    
    <Title>WeihanLi.Common</Title>
    <Description>log4net logging provider for WeihanLi.Common.Logging, and log4net extensions</Description>
    <Summary>log4net logging provider for WeihanLi.Common.Logging, and log4net extensions</Summary>
    <PackageRequireLicenseAcceptance>false</PackageRequireLicenseAcceptance>
    <PackageReleaseNotes>
      init
    </PackageReleaseNotes>    
    <Optimize Condition="'$(Configuration)'=='Release'">true</Optimize>
    <GeneratePackageOnBuild Condition="'$(Configuration)'=='Release'">True</GeneratePackageOnBuild>
    <NoWarn>$(NoWarn);1591;DE0003;</NoWarn>
  </PropertyGroup>
</Project>